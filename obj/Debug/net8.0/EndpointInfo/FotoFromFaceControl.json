{
  "openapi": "3.0.1",
  "info": {
    "title": "FotoFromFaceControl",
    "version": "1.0"
  },
  "paths": {
    "/api/DetectFaceFeatures/upload": {
      "post": {
        "tags": [
          "DetectFaceFeatures"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "File": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              },
              "encoding": {
                "File": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/DetectFaceFeatures/process": {
      "post": {
        "tags": [
          "DetectFaceFeatures"
        ],
        "parameters": [
          {
            "name": "iscropped",
            "in": "query",
            "schema": {
              "type": "boolean",
              "default": false
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ProcessRequestModel"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/ProcessRequestModel"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/ProcessRequestModel"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/SelectedFetures/process": {
      "post": {
        "tags": [
          "SelectedFetures"
        ],
        "parameters": [
          {
            "name": "cropeyes",
            "in": "query",
            "schema": {
              "type": "boolean",
              "default": false
            }
          },
          {
            "name": "cropnose",
            "in": "query",
            "schema": {
              "type": "boolean",
              "default": false
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ProcessRequestModel"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/ProcessRequestModel"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/ProcessRequestModel"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ProcessRequestModel": {
        "type": "object",
        "properties": {
          "imageUrl": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}